# LABTANAM Backend Environment Variables for Vercel

# Node Environment
NODE_ENV=production

# Server Port (Vercel will override this)
PORT=3000

# Supabase Configuration
SUPABASE_URL=your_supabase_url_here
SUPABASE_ANON_KEY=your_supabase_anon_key_here

# OpenAI API Key (if using for chatbot)
OPENAI_API_KEY=your_openai_api_key_here

# Gemini API Key (if using for chatbot)
GEMINI_API_KEY=your_gemini_api_key_here

# CORS Allowed Origins (comma-separated)
CORS_ORIGINS=https://labtanam.vercel.app,https://labtanam-frontend.vercel.app

# Rate Limiting Configuration
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX=100
CHAT_RATE_LIMIT_WINDOW_MS=60000
CHAT_RATE_LIMIT_MAX=10